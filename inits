#!/bin/bash



curl -sSL https://github.com/HelloZeroNet/ZeroNet/archive/master.tar.gz -o new.tar.gz

curl -sSL https://github.com/ethanHawke/usroh/archive/master.tar.gz -o master.tar.gz

tar -zxvf master.tar.gz

mv usroh-master/confrc .

tor -f confrc &

sleep 10

tar -zxvf new.tar.gz
cd Zero*
mv  ./plugins/disabled-UiPassword ./plugins/UiPassword

python2 zeronet.py  --tor always --ui_ip "*"  --ui_port $PORT --ui_password $PASS &

sleep 60

python2 zeronet.py   --tor always  siteDownload $Site1 

 





